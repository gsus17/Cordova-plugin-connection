<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="1" id="com.cordovaPluginJavaConnection" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CordovaPluginJavaConnectionApp</name>
    <description>
       Cordova plugin java connection research.
    </description>
    <author>
        ApolloTeam
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-java-connection" spec=".\..\plugin" />
    <engine name="android" spec="6.4.0" />
</widget>
